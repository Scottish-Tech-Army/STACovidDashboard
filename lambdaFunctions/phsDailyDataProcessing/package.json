{
  "name": "phsdailydataprocessing",
  "version": "1.0.0",
  "private": true,
  "description": "Lambda function to retrieve the PHS dailiy COVID data files and process them for COVID Dashboard use",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --watchAll",
    "build": "npx webpack --config webpack.config.js"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/eslint-plugin": "^7.17.7",
    "@babel/preset-env": "^7.13.12",
    "babel-jest": "^28.1.0",
    "eslint": "^8.15.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-webpack-plugin": "^3.1.1",
    "jest": "^28.1.0",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "aws-sdk": "^2.876.0",
    "dayjs": "^1.10.4",
    "node-fetch": "^2.6.7"
  },
  "eslintConfig": {
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "env": {
      "jest": true
    },
    "extends": [
      "plugin:jest/all"
    ],
    "plugins": [
      "jest",
      "@babel"
    ],
    "rules": {
      "jest/prefer-expect-assertions": "off",
      "jest/no-hooks": "off",
      "jest/prefer-spy-on": "off"
    }
  },
  "jest": {
    "resetMocks": false,
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": [
        "babel-jest",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "targets": {
                  "node": "current"
                }
              }
            ]
          ]
        }
      ]
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
